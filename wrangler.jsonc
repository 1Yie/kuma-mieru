{
    "name": "uptime-kuma-pages",
    "compatibility_date": "2025-05-29",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    "main": ".open-next/worker.js",
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    },
    "build": {
        "command": "bun run build:cloudflare",
        "cwd": ".",
        "watch_dir": ".open-next"
    },
    "deploy": {
        "command": "bun run deploy:cloudflare"
    }
}